   "source": [
    "PROGRAM TO PRINT FACTORS OF NUMBER TO THE INPUT NUMBER"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 13,
   "id": "bb58eb94",
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "Enter the number=12\n",
      "1 2 3 4 6 12 factorials of the number 12\n"
     ]
    }
   ],
   "source": [
    "n=int(input(\"Enter the number=\"))\n",
    "for i in range(1,n+1):\n",
    "    if n%i==0:\n",
    "        print( i, end=\" \")\n",
    "print(\"factorials of the number\",n)       \n"
   ]
